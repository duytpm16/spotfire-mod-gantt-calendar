{
    "apiVersion": "1.2",
    "version": "1.0",
    "name": "Mod starter",
    "id": "js-dev-starter",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        }
    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
        "axes": [
            {
                "name": "Task",
                "mode": "categorical",
                "placement": "none",
                "dropTarget": {
                    "icon": "XAxis3D",
                    "description": "Use {0} on the mod's X-axis"
                }
            },
            {
                "name": "Start",
                "mode": "continuous",
                "placement": "none",
                "automaticConfiguration": {
                    "expressionHeuristics": "none"
                },
                "dataTypes": {"allowNumeric": false, "allowDateTime": true, "allowString": false}
            },
            {
                "name": "End",
                "mode": "continuous",
                "placement": "none",
                "automaticConfiguration": {
                    "expressionHeuristics": "none"
                },
                "dataTypes": {"allowNumeric": false, "allowDateTime": true, "allowString": false}
            }
        ]
    },
    "files": [
        "index.html",
        "main.css",
        "main.js"
    ]
}